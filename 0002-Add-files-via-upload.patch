@@ -0,0 +1,58 @@
+from xml.dom import minidom as minidom
+from xml.dom import  Node
+import os
+import pandas as pd
+import datetime
+#import numpy as np
+'''text = 'Extra Header Start\
+Extra Header End\
+Extra Header Start\
+Extra Header End'
+with open('/home/guo/script/tscancode202105121758.xml') as lines:
+    f = lines.readlines()
+    if text in f:
+        text=''
+    lines.close()'''
+print('')
+file_name = input()
+csv_header = ['file','line','id','subid','severity','msg','web_identify','func_info','content']
+
+doc=minidom.parse(file_name) #xml
+                   
+root=doc.documentElement#
+print(root.nodeName,',',root.nodeValue,',',root.nodeType)               #
+errors=root.getElementsByTagName('error')
+total_list = []
+if errors :
+    for error in errors:
+        alist = []        
+        '''print('%s'% error.getAttribute('file'))
+        print('%s'% error.getAttribute('line'))
+        print('%s'% error.getAttribute('id'))
+        print('%s'% error.getAttribute('subid'))
+        print('%s'% error.getAttribute('severity'))
+        print('%s'% error.getAttribute('msg'))
+        print('%s'% error.getAttribute('web_identify'))
+        print('%s'% error.getAttribute('func_info'))
+        print('%s'% error.getAttribute('content'))'''
+        alist.append(error.getAttribute('file'))
+        alist.append(error.getAttribute('line'))
+        alist.append(error.getAttribute('id'))
+        alist.append(error.getAttribute('subid'))
+        alist.append(error.getAttribute('severity'))
+        alist.append(error.getAttribute('msg'))
+        alist.append(error.getAttribute('web_identify'))
+        alist.append(error.getAttribute('func_info'))
+        alist.append(error.getAttribute('content'))
+        total_list.append(alist)
+    #print(total_list)
+    #print(total_list[2])
+    df = pd.DataFrame(total_list, columns=csv_header)
+    df.to_excel(datetime.datetime.now().strftime("%Y%m%d%H%M")+'.xlsx', index=None)
+    print('')
+else:
+    print('')
+
+
+
+
